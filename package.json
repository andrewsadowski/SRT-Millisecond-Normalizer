{
  "name": "srt-ms-normalizer",
  "version": "1.0.1",
  "description": "Node CLI that checks for discrepancies in the MS portion of the timestamp from the end-time and start-time of subtitles. It then updates the MS to be continuous.",
  "url": "https://github.com/andrewsadowski/srt-millisecond-normalizer",
  "main": "lib/msNormalizer.js",
  "scripts": {
    "start": "node lib/msNormalizer.js",
    "start:watch": "nodemon lib/msNormalizer.js",
    "test": "mocha lib/msNormalizer.test.js",
    "test:watch": "nodemon --exec 'npm test'"
  },
  "bin": {
    "msNormalizer": "./bin/msNormalizer-global.js"
  },
  "preferGlobal": true,
  "author": "Andrew N Sadowski",
  "license": "ISC",
  "dependencies": {
    "expect": "^23.5.0",
    "mocha": "^5.2.0",
    "subtitles-parser": "0.0.2",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}